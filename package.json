{
  "name": "Wapcon",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "build-front": "cross-env NODE_ENV=production browserify -g envify -t vueify -e front/index.js -o build/front.js",
    "dev-front": "watchify -dv -t vueify -p browserify-hmr -e front/index.js -o build/front.js",
    "lint": "eslint --ext js,vue ./main.js front/**/*.js front/**/*.vue",
    "postinstall": "mkdir build || true",
    "run": "electron .",
    "start": "npm run lint -s & concurrently \"npm run run -s\" \"npm run dev-front -s\"",
    "test": "cross-env NODE_ENV=test mocha test/front/**/*.test.js"
  },
  "dependencies": {
    "browserify": "^14.4.0",
    "electron": "^1.6.10",
    "envify": "^4.0.0",
    "font-awesome": "^4.7.0",
    "material-design-lite": "^1.3.0",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.3",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vueify": "^9.4.1",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "browserify-hmr": "^0.3.5",
    "chai": "^4.1.2",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.0.0",
    "eslint-plugin-html": "^3.0.0",
    "mocha": "^3.5.3",
    "sinon": "^4.0.0",
    "watchify": "^3.9.0"
  }
}
